{
  "name": "celestia",
  "version": "2.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "sass:global": "node-sass -w ./docs/css/global.scss ./docs/css/global.css --output-style compressed",
    "sass:atomic": "node-sass -w ./docs/css/atomic.scss ./docs/css/atomic.css --output-style compressed",
    "start": "sirv docs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "node-sass": "4.14.1",
    "rollup": "2.33.3",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.1.1",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.30.0",
    "svelte-preprocess": "4.6.0",
    "sirv-cli": "1.0.8"
  }
}
